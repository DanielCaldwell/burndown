<% if current_user %>
  <div class="le-hook"></div>
<% end %>

<!-- Templates -->
<script type="text/template" id="tmpl_repo">
  <h1>Repos</h1>
  <input type="text" class="input-medium search-query">
  <button id="get_repo_milestones" placeholder="tracelytics/tracelons" class="btn">Search</button>
  <hr>
  <ul class="milestones">
  {{ _.each(milestones, function (milestone, i) { }}
      <li><a href="#milestone/{{=i}}">{{=milestone.get('title')}}</a></li>
  {{ }); }}
  </ul>
</script>

<script type="text/template" id="tmpl_milestone">
  <a href="#">Back</a>
  <h1>Milestone</h1>
  <div>{{=milestone.get('title')}}</div>
</script>
